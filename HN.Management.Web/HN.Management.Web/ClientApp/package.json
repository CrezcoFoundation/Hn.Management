    "@angular/platform-browser": "^16.1.2",
    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/router": "^16.1.2",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@ngrx/store": "^16.0.1",
